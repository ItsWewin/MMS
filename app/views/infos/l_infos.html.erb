<div class="col-md-12">
    <div class="box">
      <div class="box-header with-border">
        <h3 class="box-title">下级会员</h3>
      </div>
      <div class="box-body">
        <table class="table table-bordered">
          <tr>
            <th>昵称</th>
            <th>邮箱</th>
            <th>总提成</th>
            <th>提成记录</th>
          </tr>
          <% @l_users.each do |user| %>
            <tr>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%#= current_user.deduct_percentage_form_user(user.id) %></td>
              <td>
                <%= link_to '提成记录', deduct_percentages_path(user_id: user.id) %>
                <%= link_to '特价', h_set_prices_path(user_id: user.id) %>
              </td>
            </tr>
          <% end %>
        </table>
      </div>
      <div class="box-footer clearfix">
        <ul class="pagination pagination-sm inline">
        </ul>
      </div>
    </div>
</div>
